<div class="row">
  <div class="col-md-3 col-md-offset-3 ">
       
    <div class="panel panel-default">
      <div class="panel-heading"><h2>Log In</h2></div>  
      <div class="panel-body">
    <%= link_to "Sign In with Google", user_omniauth_authorize_path(:google_oauth2), class: "btn btn-primary" %>
    <%= link_to "Sign In with Facebook", user_omniauth_authorize_path(:facebook), class: "btn btn-primary" %>   
    <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <%= f.input :email  %>
      <%= f.input :password,  autocomplete: "off" %>


      
      <% if devise_mapping.rememberable? %>
        <div class="form-group">
          <%= f.check_box :remember_me %> Remember Me
        </div>
      <% end %>
    
      <div class="row">
      <div class="actions">
        <%= link_to "Forgot Password", new_user_password_path, :class => "btn btn-primary" %>
        <div class="col-xs-4">
        <%= f.submit "Log in", :class => "btn btn-primary" %>
    </div>
      </div>
    <% end %>
  </div>  
  </div>
</div>
</div>
</div>
</div>

